{
    "name": "@komemichael/komecodes",
    "version": "1.0.4",
    "description": "kome coding packages",
    "main": "index.js",
    "scripts": {
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/komemichael/komecodes.git"
    },
    "keywords": [
        "kome"
    ],
    "author": "Kome Michael",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/komemichael/komecodes/issues"
    },
    "homepage": "https://github.com/komemichael/komecodes#readme",
    "jest": {
        "verbose": true,
        "coverage": true,
        "collectCoverageFrom": [
            "src/**/*.{ts,tsx}",
            "!src/**/*.d.ts"
        ],
        "coverageDirectory": "coverage",
        "coverageReporters": [
            "text",
            "lcov"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 100,
                "functions": 100,
                "lines": 100,
                "statements": 100
            }
        },
        "preset": "ts-jest",
        "testEnvironment": "node",
        "testMatch": [
            "**/__tests__/**/*.{tss,tsxs}",
            "**/?(*.)(spec|test).{tss,tsxs}"
        ],
        "transform": {
            "^.+\\.(ts|tsx)$": "ts-jest"
        },
        "setupFilesAfterEnv": [
            "./src/test/jest.setup.ts"
        ]
    },
    "dependencies": {
        "@emotion/cache": "^11.5.0",
        "@emotion/react": "^11.5.0",
        "@emotion/server": "^11.4.0",
        "@emotion/styled": "^11.3.0",
        "@material-ui/core": "^4.12.3",
        "@material-ui/icons": "^4.11.2",
        "@material-ui/lab": "^4.0.0-alpha.60",
        "@mui/icons-material": "^5.0.5",
        "@mui/lab": "^5.0.0-alpha.58",
        "@mui/material": "^5.0.6",
        "@mui/styled-engine-sc": "^5.0.4",
        "@react-google-maps/api": "^2.5.0",
        "autosuggest-highlight": "^3.2.0",
        "body-parser": "^1.19.0",
        "cookie-session": "^1.4.0",
        "express": "^4.17.1",
        "express-async-errors": "^3.1.1",
        "express-validator": "^6.12.1",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.21",
        "moment": "^2.29.1",
        "next": "12.0.1",
        "notistack": "^2.0.3",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-swipeable-views": "^0.14.0",
        "react-swipeable-views-utils": "^0.14.0",
        "socket.io": "^4.3.1",
        "socket.io-client": "^4.3.2",
        "styled-components": "^5.3.3"
    },
    "devDependencies": {
        "@types/autosuggest-highlight": "^3.1.1",
        "@types/cookie-session": "^2.0.43",
        "@types/express": "^4.17.13",
        "@types/googlemaps": "^3.43.3",
        "@types/jest": "^27.0.2",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/lodash": "^4.14.176",
        "@types/mongoose": "^5.11.97",
        "@types/node": "16.11.6",
        "@types/react": "17.0.33",
        "@types/react-swipeable-views": "^0.13.1",
        "@types/react-swipeable-views-utils": "^0.13.3",
        "@types/styled-components": "^5.1.15",
        "axios": "^0.21.4",
        "babel-plugin-styled-components": "^1.13.3",
        "eslint": "7.32.0",
        "eslint-config-next": "12.0.1",
        "jest": "^27.3.1",
        "ts-jest": "^27.0.7",
        "ts-node": "^10.4.0",
        "typescript": "4.4.4"
    }
}
